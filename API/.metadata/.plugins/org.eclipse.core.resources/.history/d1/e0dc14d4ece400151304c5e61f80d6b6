
public class Main {
	public  String StringReverse(String input)
	{
		String output = "";
		String tokensArray[]= input.split(" ");
		System.out.println(tokensArray.length);
		System.out.println(tokensArray[2]);
		for(int i=tokensArray.length-1;i>=0;i--)
		{
			output+=tokensArray[i]+" ";
			
		}

	return output;
	}
	
	public static void main(String[] args)
	{
		Main a = new Main();
		 Scanner sc=new Scanner(System.in);
		System.out.println(a.StringReverse("Alice like Cats"));
	}
}
