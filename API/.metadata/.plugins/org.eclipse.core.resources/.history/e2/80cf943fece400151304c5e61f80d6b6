
public class Main {
	public static String StringReverse(String input)
	{
		String output = null;
		String tokensArray[]= input.split(" ");
		System.out.println(tokensArray.length);
		for(int i=tokensArray.length;i>0;i--)
		{
			output+=tokensArray[i];
			System.out.println(tokensArray[i]);
		}

	return output;
	}
	
	public static void main(String[] args)
	{
		
		System.out.println(StringReverse("Alice like Cats"));
	}
}
