
public class Main {
	public  String StringReverse(String input)
	{
		String output = null;
		String tokensArray[]= input.split(" ");
		System.out.println(tokensArray.length);
		System.out.println(tokensArray[3]);
		for(int i=tokensArray.length;i>0;i--)
		{
			output+=tokensArray[i];
			
		}

	return output;
	}
	
	public static void main(String[] args)
	{
		Main a = new Main();
		System.out.println(StringReverse("Alice like Cats"));
	}
}
